{"ast":null,"code":"// import blogs from '../components/blogs.vue'\n// import testemonials from '../components/testemonials.vue'\n// import best_seller_slides from '../components/best-seller-slides.vue'\nimport Faqs from '../components/Faqs.vue';\nimport axios from 'axios'; // @ is an alias to /src\n// import HelloWorld from '@/components/HelloWorld.vue'\n// import HelloWorld from '@/components/blogs.vue'\n\nexport default {\n  name: 'HomeView',\n\n  data() {\n    return {\n      allProducts: [],\n      allCategories: [],\n      category: this.$route.params.category\n    };\n  },\n\n  components: {\n    // HelloWorld\n    // blogs,\n    // testemonials,\n    // best_seller_slides,\n    Faqs\n  },\n\n  mounted() {\n    this.getAllProducts();\n    this.getAllCategories();\n  },\n\n  methods: {\n    getAllProducts() {\n      axios.get('/api/v1/category/' + this.category).then(response => {\n        this.allProducts = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    getAllCategories() {\n      axios.get('/api/v1/category/').then(response => {\n        this.allCategories = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyDA;AACA;AACA;AACA,OAAOA,IAAP,MAAiB,wBAAjB;AACA,OAAOC,KAAP,MAAkB,OAAlB,C,CACA;AACA;AACA;;AACA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAE;IACF,OAAO;MACHC,WAAW,EAAC,EADT;MAEHC,aAAa,EAAC,EAFX;MAGHC,QAAQ,EAAE,KAAKC,MAAL,CAAYC,MAAZ,CAAmBF;IAH1B,CAAP;EAKH,CARY;;EASbG,UAAU,EAAE;IACV;IACA;IACA;IACA;IACAT;EALU,CATC;;EAgBbU,OAAO,GAAE;IACP,KAAKC,cAAL;IACA,KAAKC,gBAAL;EACD,CAnBY;;EAoBbC,OAAO,EAAE;IACLF,cAAc,GAAG;MACbV,KAAK,CAACa,GAAN,CAAU,sBAAsB,KAAKR,QAArC,EACCS,IADD,CACMC,QAAQ,IAAE;QACd,KAAKZ,WAAL,GAAiBY,QAAQ,CAACb,IAA1B;MACD,CAHD,EAICc,KAJD,CAIOC,KAAK,IAAE;QACVC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACH,CAND;IAOH,CATI;;IAULN,gBAAgB,GAAG;MACjBX,KAAK,CAACa,GAAN,CAAU,mBAAV,EACGC,IADH,CACQC,QAAQ,IAAE;QACd,KAAKX,aAAL,GAAmBW,QAAQ,CAACb,IAA5B;MACD,CAHH,EAIGc,KAJH,CAISC,KAAK,IAAE;QACVC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACH,CANH;IAOF;;EAlBK;AApBI,CAAf","names":["Faqs","axios","name","data","allProducts","allCategories","category","$route","params","components","mounted","getAllProducts","getAllCategories","methods","get","then","response","catch","error","console","log"],"sourceRoot":"","sources":["/home/fiaz/Documents/GitHub/gtl-vue/src/views/category.vue"],"sourcesContent":["<template>\n    <section class=\"\">\n        <div style=\"text-align: center;\" class=\"m-5\">\n            <h1 style=\"font-family: fantasy;\" class=\"heading\"> Order now </h1>\n        </div>\n\n        <!-- container -->\n        <div class=\"ml-3\">\n            <div class=\"row\">\n                \n                <div class=\"col-3 \" style=\"text-align: left;\">\n                    <h4 style=\"font-family: fantasy;\" class=\"heading \"> Category </h4>\n                    <ul class=\"categories\" style=\"font-family: Impact, Haettenschweiler, sans-serif;\">\n                        <!-- list of Categories -->\n                        <li class='mt-2' v-for=\"item in this.allCategories\" :key=\"item.id\">\n                            <a :href=\"'/categories/' + item.name_without_space \" > {{item.title}} </a>\n                        </li>\n\n                    </ul>\n                </div>\n\n                <div class=\"col-9 \">\n                  <div v-if=\"!this.allProducts.length \">\n                    <h1 class=\"heading m-5\">Sorry we are updating our website </h1>\n                    <p class=\"m-5\"><strong>PLEASE Call Toll- Free 1 (833) ANASAZI to place an order.</strong></p>\n                  </div>\n                  <div v-else>\n                    <!-- Row -->\n                    <div class=\"row\">\n                        <!-- Card -->\n                        <div class=\"card m-2 \" style=\"width: 18rem;\" v-for=\"item in this.allProducts\" :key=\"item.id\">\n                            <img :src= item.get_image  style=\"height: 350px\" alt=\"...\">\n                            <div class=\"card-body\">\n                                <h5 class=\"card-title\"><b> {{item.name}}</b></h5>\n                                <p class=\"card-text\">{{item.priceRange}}</p>\n                                <router-link :to=\"'/products/'+item.id\" class=\"btn btn btn-success\">Quick view</router-link>\n                            </div>\n                        </div>\n\n                    </div>\n                  </div>\n                </div>\n\n            </div>\n        </div>\n\n        <!--  Faqs-->\n        <Faqs/>\n\n    </section>\n\n\n</template>\n\n\n\n<script>\n// import blogs from '../components/blogs.vue'\n// import testemonials from '../components/testemonials.vue'\n// import best_seller_slides from '../components/best-seller-slides.vue'\nimport Faqs from '../components/Faqs.vue'\nimport axios from 'axios'\n// @ is an alias to /src\n// import HelloWorld from '@/components/HelloWorld.vue'\n// import HelloWorld from '@/components/blogs.vue'\nexport default {\n  name: 'HomeView',\n  data(){\n      return {\n          allProducts:[],\n          allCategories:[],\n          category: this.$route.params.category\n      }\n  },\n  components: {\n    // HelloWorld\n    // blogs,\n    // testemonials,\n    // best_seller_slides,\n    Faqs\n  },\n  mounted(){\n    this.getAllProducts()\n    this.getAllCategories()\n  },\n  methods: {\n      getAllProducts() {\n          axios.get('/api/v1/category/' + this.category)\n          .then(response=>{\n            this.allProducts=response.data\n          })\n          .catch(error=>{\n              console.log(error)\n          })\n      },\n      getAllCategories() {\n        axios.get('/api/v1/category/')\n          .then(response=>{\n            this.allCategories=response.data\n          })\n          .catch(error=>{\n              console.log(error)\n          })\n      }\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}