{"ast":null,"code":"// import blogs from '../components/blogs.vue'\n// import axios from 'axios'\n// import testemonials from '../components/testemonials.vue'\n// import best_seller_slides from '../components/best-seller-slides.vue'\n// @ is an alias to /src\n// import HelloWorld from '@/components/HelloWorld.vue'\nimport CartItem from '@/components/CartItem.vue';\nexport default {\n  name: 'Cart_',\n  components: {\n    CartItem // blogs,\n\n  },\n\n  data() {\n    return {\n      cart: {\n        items: []\n      },\n      userid: 0\n    };\n  },\n\n  mounted() {\n    this.cart = this.$store.state.cart;\n    this.userid = localStorage.getItem('userid'); //   console.log(this.$store.state.userid)\n    //   console.log(localStorage.getItem('userid'))\n  },\n\n  methods: {\n    removeFromCart(item) {\n      this.cart.items = this.cart.items.filter(i => i.product.id !== item.product.id);\n    }\n\n  },\n  computed: {\n    cartTotalLength() {\n      return this.cart.items.reduce((acc, curVal) => {\n        return acc += curVal.quantity;\n      }, 0);\n    },\n\n    cartTotalPrice() {\n      return this.cart.items.reduce((acc, curVal) => {\n        return acc += curVal.quantity * curVal.price;\n      }, 0);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6EA;AACA;AACA;AACA;AACA;AACA;AACA,OAAOA,QAAP,MAAqB,2BAArB;AACA,eAAe;EACXC,IAAI,EAAE,OADK;EAEXC,UAAU,EAAE;IACRF,QADQ,CAER;;EAFQ,CAFD;;EAMXG,IAAI,GAAG;IACH,OAAO;MACHC,IAAI,EAAE;QACFC,KAAK,EAAE;MADL,CADH;MAIHC,MAAM,EAAC;IAJJ,CAAP;EAMH,CAbU;;EAcXC,OAAO,GAAG;IACR,KAAKH,IAAL,GAAY,KAAKI,MAAL,CAAYC,KAAZ,CAAkBL,IAA9B;IACA,KAAKE,MAAL,GAAcI,YAAY,CAACC,OAAb,CAAqB,QAArB,CAAd,CAFQ,CAGV;IACA;EACC,CAnBU;;EAoBXC,OAAO,EAAE;IACLC,cAAc,CAACC,IAAD,EAAO;MACjB,KAAKV,IAAL,CAAUC,KAAV,GAAkB,KAAKD,IAAL,CAAUC,KAAV,CAAgBU,MAAhB,CAAuBC,KAAKA,CAAC,CAACC,OAAF,CAAUC,EAAV,KAAiBJ,IAAI,CAACG,OAAL,CAAaC,EAA1D,CAAlB;IACJ;;EAHK,CApBE;EAyBXC,QAAQ,EAAE;IACNC,eAAe,GAAG;MACd,OAAO,KAAKhB,IAAL,CAAUC,KAAV,CAAgBgB,MAAhB,CAAuB,CAACC,GAAD,EAAMC,MAAN,KAAiB;QAC3C,OAAOD,GAAE,IAAKC,MAAM,CAACC,QAArB;MACH,CAFM,EAEJ,CAFI,CAAP;IAGH,CALK;;IAMNC,cAAc,GAAG;MACb,OAAO,KAAKrB,IAAL,CAAUC,KAAV,CAAgBgB,MAAhB,CAAuB,CAACC,GAAD,EAAMC,MAAN,KAAiB;QAC3C,OAAOD,GAAE,IAAKC,MAAM,CAACC,QAAP,GAAkBD,MAAM,CAACG,KAAvC;MACH,CAFM,EAEJ,CAFI,CAAP;IAGH;;EAVK;AAzBC,CAAf","names":["CartItem","name","components","data","cart","items","userid","mounted","$store","state","localStorage","getItem","methods","removeFromCart","item","filter","i","product","id","computed","cartTotalLength","reduce","acc","curVal","quantity","cartTotalPrice","price"],"sourceRoot":"","sources":["/home/fiaz/Documents/GitHub/gtl-vue/src/views/cart.vue"],"sourcesContent":["<template>\n    <div class=\"page-cart mt-0 mb-0\">\n        <div class=\"\">\n            <!-- {{userid}} userid -->\n            <!-- <div style=\"height:100px\" class=\"invisible\">\n            </div> -->\n\n            <div class=\"mt-3 p-3\">\n                <h1 > <b>  Cart</b></h1>\n            </div>\n\n          \n            <div class=\"box m-4 p-5\">\n                <table class=\"table is-fullwidth\" v-if=\"cartTotalLength\">\n                    <thead>\n                        <tr class>\n                            <th>Product</th>\n                            <th>size</th>\n                            <th>Price</th>\n                            <th>Quantity</th>\n                            <th>Total</th>\n                            <th></th>\n                        </tr>\n                    </thead>\n\n\n\n                    <tbody>\n                        <CartItem\n                            v-for=\"item in cart.items\"\n                            v-bind:key=\"item.product.id\"\n                            v-bind:initialItem=\"item\"\n                            v-on:removeFromCart=\"removeFromCart\" />\n                    </tbody>\n                </table>\n\n                <p v-else class=\"m-2\">You don't have any products in your cart...</p>\n            </div>\n\n            <!-- {{userid}} userid -->\n            <!-- <div style=\"height:100px\" class=\"invisible \">\n            </div> -->\n\n            <div class=\" box m-4 p-4\" style=\"text-align:left\">\n              <!-- <hr> -->\n              <div class=\"row ml-3\">\n                <h2 class=\"subtitle mt-3\"><b>  Summary</b></h2>\n              </div>\n              <div class=\" ml-5\">\n                  <div class=\"row\">\n                      <div class=\"col-2\">\n                        <b>Price </b>\n                      </div>\n                      <div class=\"col\">\n                        $ {{ cartTotalPrice }}\n                      </div>\n                  </div>\n                  <div class=\"row\">\n                      <div class=\"col-2\">\n                        <b>Total Items</b>\n                      </div>\n                      <div class=\"col\">\n                        {{ cartTotalLength }}\n                      </div>\n                  </div>\n              </div>\n              <div class=\"row ml-5\">\n                <a href=\"/checkout\" class=\"btn btn btn-success mt-3 mb-3\" style=\"color: white\">Proceed to checkout</a>\n              </div>\n\n            </div>\n\n        </div>\n    </div>\n</template>\n\n<script>\n// import blogs from '../components/blogs.vue'\n// import axios from 'axios'\n// import testemonials from '../components/testemonials.vue'\n// import best_seller_slides from '../components/best-seller-slides.vue'\n// @ is an alias to /src\n// import HelloWorld from '@/components/HelloWorld.vue'\nimport CartItem from '@/components/CartItem.vue'\nexport default {\n    name: 'Cart_',\n    components: {\n        CartItem,\n        // blogs,\n    },\n    data() {\n        return {\n            cart: {\n                items: []\n            },\n            userid:0\n        }\n    },\n    mounted() {\n      this.cart = this.$store.state.cart\n      this.userid = localStorage.getItem('userid')\n    //   console.log(this.$store.state.userid)\n    //   console.log(localStorage.getItem('userid'))\n    },\n    methods: {\n        removeFromCart(item) {\n            this.cart.items = this.cart.items.filter(i => i.product.id !== item.product.id)\n        }\n    },\n    computed: {\n        cartTotalLength() {\n            return this.cart.items.reduce((acc, curVal) => {\n                return acc += curVal.quantity\n            }, 0)\n        },\n        cartTotalPrice() {\n            return this.cart.items.reduce((acc, curVal) => {\n                return acc += curVal.quantity * curVal.price\n            }, 0)\n        },\n    }\n}\n</script>\n\n<style scoped>\n.box{\n   box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);\n   height:100%;\n}\n/* max 800px or less */\n@media only screen and (max-width: 936px) {\n  .invisible{\n    display:none;\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}