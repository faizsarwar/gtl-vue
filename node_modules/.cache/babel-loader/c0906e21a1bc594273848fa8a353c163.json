ity;\n      }\n\n      return totalLength;\n    }\n\n  }\n};\ndocument.addEventListener(\"DOMContentLoaded\", function () {\n  window.addEventListener(\"scroll\", function () {\n    if (window.scrollY > 50) {\n      document.getElementById(\"navbar_top\").classList.add(\"fixed-top\"); // add padding top to show content behind navbar\n      // document.body.style.paddingTop = navbar_height + 'px';\n    } else {\n      document.getElementById(\"navbar_top\").classList.remove(\"fixed-top\"); // remove padding top from body\n\n      document.body.style.paddingTop = \"0\";\n    }\n  });\n});","map":{"version":3,"mappings":"AAykBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,IAAP,MAAiB,aAAjB;AACA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,cAAc,EAAE,KADX;MAELC,KAAK,EAAE,EAFF;MAGLC,MAAM,EAAE,EAHH;MAILC,IAAI,EAAE;QACJC,KAAK,EAAE;MADH;IAJD,CAAP;EAQD,CAVY;;EAWbC,YAAY,GAAG;IACb,KAAKC,MAAL,CAAYC,MAAZ,CAAmB,iBAAnB;IACA,MAAMC,KAAI,GAAI,KAAKF,MAAL,CAAYG,KAAZ,CAAkBD,KAAhC;;IACA,IAAIA,KAAJ,EAAW;MACTX,KAAK,CAACa,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,eAA9B,IAAiD,WAAWJ,KAA5D;IACF,CAFA,MAEO;MACLX,KAAK,CAACa,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,eAA9B,IAAiD,EAAjD;IACF;EACD,CAnBY;;EAoBbC,OAAO,GAAG;IACR,KAAKV,IAAL,GAAY,KAAKG,MAAL,CAAYG,KAAZ,CAAkBN,IAA9B;EACD,CAtBY;;EAuBbW,OAAO,EAAE;IACPC,MAAM,GAAG;MACPlB,KAAK,CAACa,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,eAA9B,IAAiD,EAAjD;MACAI,YAAY,CAACC,UAAb,CAAwB,OAAxB;MACAD,YAAY,CAACC,UAAb,CAAwB,UAAxB;MACAD,YAAY,CAACC,UAAb,CAAwB,QAAxB;MACA,KAAKX,MAAL,CAAYC,MAAZ,CAAmB,aAAnB;MACA,KAAKD,MAAL,CAAYC,MAAZ,CAAmB,cAAnB;MACA,KAAKW,OAAL,CAAaC,IAAb,CAAkB,GAAlB;IACD,CATM;;IAUP,MAAMC,UAAN,GAAmB;MACjB,MAAMC,QAAO,GAAI;QACfpB,KAAK,EAAE,KAAKA;MADG,CAAjB;MAGA,MAAMJ,KAAI,CACPyB,IADG,CACE,eADF,EACmBD,QADnB,EAEHE,IAFG,CAEGC,QAAD,IAAc;QAClBC,OAAO,CAACC,GAAR,CAAYF,QAAZ;QACA1B,IAAI,CAAC6B,IAAL,CAAU;UACRC,QAAQ,EAAE,SADF;UAERC,IAAI,EAAE,SAFE;UAGRC,KAAK,EAAE,+BAHC;UAIRC,iBAAiB,EAAE,KAJX;UAKRC,KAAK,EAAE;QALC,CAAV;MAOD,CAXG,EAYHC,KAZG,CAYIC,KAAD,IAAW;QAChB,IAAIA,KAAK,CAACV,QAAV,EAAoB;UAClB,KAAK,MAAMW,QAAX,IAA