div class=\"card-body\" style=\"height: 16rem;\">\n                  <h5 class=\"card-title\"><b> {{item.name}}</b></h5>\n                  <p class=\"card-text\"><b> ${{item.price}} </b></p>\n                  <a :href=\"'/products/' + item.id \" class=\"btn btn btn-success\">Quick view</a>\n              </div>\n          </div>\n      </div>\n    </div>\n  \n  </section>\n</template>\n\n\n<script>\nimport axios from 'axios'\nimport Loading from 'vue-loading-overlay';\nimport 'vue-loading-overlay/dist/vue-loading.css';\nexport default {\nname: 'cbd_oil',\n  data(){\n      return {\n          allProducts:[],\n          allCategories:[],\n          isLoading: true,\n          fullPage: true,\n          bars: \"bars\",\n          color:\"#07ad31\",\n          height: \"120\",\n          width: \"120\",\n          account_details:[]\n      }\n  },\n  components: {\n    Loading,\n  },\n    mounted(){\n    this.getAllProducts()\n    this.getAccountDetails()\n  },\n  methods: {\n      async getAllProducts() {\n          this.$store.commit('setIsLoading', true)\n          axios.get('/api/v1/category/cbd_oil/')\n          .then(response=>{\n            this.allProducts=response.data\n          })\n          .catch(error=>{\n              console.log(error)\n          })\n          this.$store.commit('setIsLoading', false)\n      },\n      getAccountDetails(){\n        axios.get(`/api/v1/CustomUser/${localStorage.getItem('userid')}/`)\n          .then(response=>{\n            this.account_details=response.data\n          })\n          .catch(error=>{\n              console.log(error)\n          })\n      }\n  }\n\n}\n</script>\n<style scoped>\na{\n    color: white;\n    font-weight: bolder;\n}\n.form-label {\n  /* display: inline-block;\n  width: 140px; */\n  margin-bottom: 2px;\n  text-align: left;\n  color: blue;\n}\np{\n    color: black;\n}\n\nh2 b {\n    color: black;\n}\n\n</style>\n"]},"metadata":{},"sourceType":"module"}