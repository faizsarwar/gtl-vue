{"ast":null,"code":"import axios from 'axios';\nimport Loading from 'vue-loading-overlay';\nimport 'vue-loading-overlay/dist/vue-loading.css';\nexport default {\n  name: 'cbd_oil',\n\n  data() {\n    return {\n      allProducts: [],\n      allCategories: [],\n      isLoading: true,\n      fullPage: true,\n      bars: \"bars\",\n      color: \"#07ad31\",\n      height: \"120\",\n      width: \"120\",\n      account_details: []\n    };\n  },\n\n  components: {\n    Loading\n  },\n\n  mounted() {\n    this.getAllProducts();\n    this.getAccountDetails();\n  },\n\n  methods: {\n    async getAllProducts() {\n      this.$store.commit('setIsLoading', true);\n      axios.get('/api/v1/category/cbd_oil/').then(response => {\n        this.allProducts = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n      this.$store.commit('setIsLoading', false);\n    },\n\n    getAccountDetails() {\n      axios.get(`/api/v1/CustomUser/${localStorage.getItem('userid')}/`).then(response => {\n        this.account_details = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,OAAP,MAAoB,qBAApB;AACA,OAAO,0CAAP;AACA,eAAe;EACfC,IAAI,EAAE,SADS;;EAEbC,IAAI,GAAE;IACF,OAAO;MACHC,WAAW,EAAC,EADT;MAEHC,aAAa,EAAC,EAFX;MAGHC,SAAS,EAAE,IAHR;MAIHC,QAAQ,EAAE,IAJP;MAKHC,IAAI,EAAE,MALH;MAMHC,KAAK,EAAC,SANH;MAOHC,MAAM,EAAE,KAPL;MAQHC,KAAK,EAAE,KARJ;MASHC,eAAe,EAAC;IATb,CAAP;EAWH,CAdY;;EAebC,UAAU,EAAE;IACVZ;EADU,CAfC;;EAkBXa,OAAO,GAAE;IACT,KAAKC,cAAL;IACA,KAAKC,iBAAL;EACD,CArBY;;EAsBbC,OAAO,EAAE;IACL,MAAMF,cAAN,GAAuB;MACnB,KAAKG,MAAL,CAAYC,MAAZ,CAAmB,cAAnB,EAAmC,IAAnC;MACAnB,KAAK,CAACoB,GAAN,CAAU,2BAAV,EACCC,IADD,CACMC,QAAQ,IAAE;QACd,KAAKlB,WAAL,GAAiBkB,QAAQ,CAACnB,IAA1B;MACD,CAHD,EAICoB,KAJD,CAIOC,KAAK,IAAE;QACVC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACH,CAND;MAOA,KAAKN,MAAL,CAAYC,MAAZ,CAAmB,cAAnB,EAAmC,KAAnC;IACH,CAXI;;IAYLH,iBAAiB,GAAE;MACjBhB,KAAK,CAACoB,GAAN,CAAW,sBA